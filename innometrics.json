{
    "version_name": "Alchemy application",
    "version_description": "",
    "pages": [{
        "name": "App Settings",
        "type": "bundled",
        "content": "frontend/settings.html"
    }],
    "widgets": {
        "interests": {
            "title": "Interests",
            "settings": {
                "type": "bundled",
                "content": "settings/settings.html",
                "defaultSettings": {
                    "chartType": "bar"
                },
                "size": {
                    "height": 560
                }
            }
        }
    },
    "permissions": ["stream", "profile.update", "profile.read", "app.read.own", "app.update.own"],
    "runtime": {
        "language": "node",
        "version": "0.10"
    }
}
